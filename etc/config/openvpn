config openvpn 'custom_config'
        option enabled '1'
        option remote 'server1.wificonnexion.com'
        option port '8181'
        option client '1'
        option dev 'tun'
        option proto 'tcp'
        option resolv_retry 'infinite'
        option nobind '1'
        option persist_key '1'
        option persist_tun '1'
        option comp_lzo 'yes'
        option verb '2'
        option script_security '2'
        option cd '/etc/openvpn'
        option ca 'ca.crt'
        option tls_auth 'ta.key 1'
        option remote_cert_tls 'server'
        option auth_user_pass 'id'
        option route_up 'up.sh'
        option route_pre_down 'down.sh'
