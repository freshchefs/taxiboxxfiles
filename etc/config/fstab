config 'global'
	option	anon_swap	'0'
	option	anon_mount	'0'
	option	auto_swap	'1'
	option	auto_mount	'1'
	option	delay_root	'5'
	option	check_fs	'0'

config 'swap'
	option	device		'/dev/mmcblk0p1'
	option	label		'SWAP'
	option	enabled		'1'

config 'mount'
	option	device		'/mnt/mmcblk0p2'
	option	label		'DATA'
	option	fstype		'ext4'
	option	options		'rw,noatime'
	option	target		'/mnt/flashdrive'
	option	enabled_fsck	'1'
	option	enabled		'1'

